{
  "printWidth": 120,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "endOfLine": "lf",
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "strict",
  "singleAttributePerLine": true,
  "bracketSameLine": true,
  "arrowParens": "avoid",
  "proseWrap": "always"
}
