# .env.example
#
# Copy this file to .env (for development) or .env.production (for production) and fill in your values.
# These variables are loaded automatically by the build scripts.

# Development environment (local):
ASSET_URL=/dist/img
BASE_URL=/
ROBOTS=noindex,nofollow

# Alternate environment (alt):
ASSET_URL=https://iknittheweb.github.io/dist/img
BASE_URL=https://iknittheweb.github.io
ROBOTS=noindex,nofollow

# Alternate environment (netlify-alt):
ASSET_URL=https://iknittheweb.netlify.app/dist/img
BASE_URL=https://iknittheweb.netlify.app
ROBOTS=noindex,nofollow

# For production (prod):
ASSET_URL=https://iknittheweb.com/dist/img
BASE_URL=https://iknittheweb.com
ROBOTS=index,follow

# Default values for meta tags (can be overridden in individual templates)
ASSET_URL=/dist/img
BASE_URL=/
GOOGLE_FONTS_LINK=https://fonts.googleapis.com/css?family=Merriweather:400,700|Merriweather+Sans:400,700&display=swap
OG_TYPE=website
ROBOTS=noindex,nofollow
TWITTER_CARD=summary_large_image
